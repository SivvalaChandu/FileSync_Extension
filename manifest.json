{
  "manifest_version": 3,
  "name": "FileSync",
  "version": "1.0",
  "description": "Share data to other users.",
  "permissions": ["activeTab", "scripting"],
  "action": {
    "default_popup": "post/post.html",
    "default_icon": {
      "16": "icons/icon16.png",
      "32": "icons/icon32.png",
      "48": "icons/icon48.png",
      "128": "icons/icon128.png"
    }
  },
  "background": {
    "service_worker": "background/background.js"
  }
}
